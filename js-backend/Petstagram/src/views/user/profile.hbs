<div class='profile'>
  <div class='profile-data'>
    <div class='profile_img'>
      <div class='image'>
        <img src='/images/person.png' alt='img8' />
      </div>
    </div>
    <div class='personal'>
      <div class='edit'>
        <p>{{user.username}}</p>
      </div>
      <div class='data'>
        <span>{{count}}</span>
        <p>photos</p>
      </div>
      <p class='email'>{{user.email}}</p>
    </div>
  </div>
  <div class='pet-photos'>
    {{#if count}}
      {{#each photos}}
        <a href='/photos/details/{{_id}}'>
          <img src='{{image}}' alt='pet img' />
        </a>
      {{/each}}
    {{else}}
      <article class='not-available-photo'>
        <h1>You haven't uploaded photos yetâ€¦</h1>
      </article>
    {{/if}}
  </div>
</div>